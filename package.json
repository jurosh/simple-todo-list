{
  "name": "simple-todo-list-toptalacademy",
  "version": "1.0.0",
  "private": true,
  "main": "./node_modules/react-native-scripts-ts/build/bin/crna-entry.js",
  "scripts": {
    "startc": "npm start -- --reset-cache",
    "start": "cross-env REACT_NATIVE_PACKAGER_HOSTNAME='192.168.1.16' react-native-scripts-ts start",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "eject": "react-native-scripts-ts eject",
    "android": "react-native-scripts-ts android",
    "ios": "react-native-scripts-ts ios",
    "test": "node node_modules/jest/bin/jest.js"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.jsx?$": "./node_modules/babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    }
  },
  "dependencies": {
    "expo": "^26.0.0",
    "firebase": "^4.6.2",
    "react": "16.3.0-alpha.1",
    "react-native": "^0.54.4",
    "react-native-check-box": "^2.1.0",
    "react-native-image-preview": "^0.3.1",
    "react-navigation": "^1.5.8",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "tslib": "^1.9.0"
  },
  "devDependencies": {
    "@types/expo": "^25.0.2",
    "@types/expo__vector-icons": "^6.2.1",
    "@types/jest": "^22.2.0",
    "@types/react": "^16.0.38",
    "@types/react-native": "^0.52.20",
    "@types/react-navigation": "^1.2.3",
    "@types/react-test-renderer": "^16.0.1",
    "cross-env": "^5.1.4",
    "jest-expo": "^26.0.0",
    "prettier": "^1.11.1",
    "react-native-scripts-ts": "^1.13.0",
    "react-native-typescript-transformer": "^1.2.3",
    "react-test-renderer": "16.2.0",
    "ts-jest": "^22.4.2",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.5.1",
    "typescript": "^2.8.1"
  }
