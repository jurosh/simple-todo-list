{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "member-access": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-shadowed-variable": false,
    "no-console": ["error", { "allow": ["warn", "error"] }],
    "jsx-no-lambda": false
  },
  "rulesDirectory": []
}
